{
	"version": "2.0.0",
	"tasks": [
		{
			"label": "Dev server",
			"type": "shell",
			"command": "npm run dev",
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "Install deps",
			"type": "shell",
			"command": "npm install",
			"group": "build"
		},
		{
			"label": "Rebuild Next dev",
			"type": "shell",
			"command": "npm run dev",
			"isBackground": true,
			"problemMatcher": [
				"$eslint-stylish"
			],
			"group": "build"
		},
		{
			"label": "Restart dev server",
			"type": "shell",
			"command": "npm run dev",
			"isBackground": true,
			"group": "build"
		}
	]
}